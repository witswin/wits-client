{
  "name": "wits-client",
  "version": "0.0.1",
  "description": "A client for using wits.win apis",
  "license": "MIT",
  "homepage": "https://wits.win",
  "repository": "github:witswin/wits-client",
  "bugs": "https://github.com/witswin/wits-client/issues",
  "packageManager": "bun@1.2.7",
  "authors": [
    "Bob six (https://github.com/Bob-six)"
  ],
  "keywords": [
    "wits-client",
    "wits",
    "api"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "lint": "eslint --ext js,ts src test",
    "build": "bun clean && tsup",
    "create-types:user": "openapi-typescript ./wits-api.yaml -o src/api-client.d.ts",
    "create-types:dashboard": "openapi-typescript ./wits-dashboard-api.json -o src/api-dashboard.d.ts",
    "create-types": "bun create-types:user && bun create-types:dashboard",
    "prepublishOnly": "bun clean && bun format:check && bun lint && bun test",
    "prepack": "bun run build",
    "examples:lint": "eslint --ext js,ts examples",
    "tsc": "tsc"
  },
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@types/babel__core": "^7.20.5",
    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/bun": "latest",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "cross-env": "^7.0.3",
    "esbuild-extra": "^0.3.1",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.10",
    "netlify-plugin-cache": "^1.0.3",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "tsup": "8.0.2",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
